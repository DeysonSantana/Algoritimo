Algoritmo "Creuza02"
// Disciplina   : [Lógica de Programação]
// Professor   :
// Descrição   :
// Autor(a)    :
// Data atual  :
Var
   reais, dolar, dinheiro: real



Inicio
       Escreva("Quantos reais eu tenho? ")
       leia(reais)
       Escreva("Quanto vale o dolar? ")
       leia(dolar)
       dinheiro <- reais / dolar
       Escreval("Você tem",dinheiro,"dolares.")


Fimalgoritmo